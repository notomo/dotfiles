
[core]
    excludesfile = ~/.gitignore_global
    editor = nvim
    quotepath = false

[user]
    name = 
    email = 

[url "git@github.com:"]
    pushInsteadOf = git://github.com/
    pushInsteadOf = https://github.com/

[alias]
    pushfff = push --force-with-lease --force-if-includes
    rc = rebase --continue
    rs = rebase --skip
[init]
    defaultBranch = main
