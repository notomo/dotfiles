snippet ifonly
	if (${1:condition}) {
		${0:TARGET}
	}

snippet ifelse
	if (${1:condition}) {
		${2:TARGET}
	} else {
		${0:TARGET}
	}

snippet else
	else {
		${0:TARGET}
	}

snippet for
	for (uint32_t i = 0; i < ${1:condition}; i++) {
		${0:TARGET}
	}

snippet continue
	continue;

snippet funciton
	${1:void} ${2:name}()
	{
		${0:TARGET}
	}
