snippet slice
abbr slice := []type{value, ...}
	${1:name} := []${2:typeName}{${0:value}}

snippet map
abbr map := map[keyType]valueType{key: value, ...}
	${1:name} := map[${2:keyTypeName}]${3:valueTypeName}{${4:key}: ${0:name},}

snippet const
abbr const name = value
