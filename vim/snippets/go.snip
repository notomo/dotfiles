snippet var
abbr var names type
	var ${1:names} ${0:type}

snippet var_default
abbr var names default_value
	var ${1:names} = ${0:value}

snippet struct_init
	${1:name} := ${2:structName}{${3:key}: ${0:value}}

snippet array
abbr array := [...]type{value, ...}
